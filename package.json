{
  "name": "goldfishjs",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "ci": "npm run build && npm run lint && npm run test",
    "release": "node scripts/release.js",
    "lint": "node scripts/lint.js",
    "build": "node scripts/build.js",
    "reboot": "rm -rf node_modules && npm i --no-package-lock && node scripts/reboot.js",
