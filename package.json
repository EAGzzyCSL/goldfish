    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-prettier": "3.1.4",
    "lerna": "3.22.1",
    "typescript": "3.9.5"
  },
  "dependencies": {
    "@types/jest": "24.0.22",
    "vuepress": "1.5.2",
    "@vuepress/plugin-back-to-top": "1.4.0",
    "@vuepress/plugin-medium-zoom": "1.5.2"
  }
